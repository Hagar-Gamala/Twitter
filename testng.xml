<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Twitter Regression Suite">
    <parameter name="browser" value="chrome" />
    <test name="Regression Test" parallel="false" preserve-order="true">
        <classes>
            <class name="tests.LoginTest"/>
            <class name="tests.PollTest"/>
            <class name="tests.TweetTest"/>
            <class name="tests.ReplyTest"/>
        </classes>
    </test>
</suite>
